		"lodash": "^4.17.4",
		"native-base": "^2.0.13",
		"react": "~15.4.1",
		"react-native": "0.42.3",
		"react-native-linear-gradient": "^2.0.0",
		"react-native-splash-screen": "^2.0.0",
		"react-native-storage": "^0.1.5",
		"react-navigation": "^1.0.0-beta.7",
		"react-redux": "^5.0.3",
		"redux": "^3.6.0",
		"redux-logger": "^2.8.2",
		"redux-thunk": "^2.2.0"
	},
	"devDependencies": {
		"babel-eslint": "^7.1.1",
		"babel-jest": "19.0.0",
		"babel-loader": "^6.4.1",
